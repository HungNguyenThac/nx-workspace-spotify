{
  "projectType": "library",
  "sourceRoot": "libs/web/shared/ui/input/src",
  "prefix": "as",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/web/shared/ui/input/src/**/*.ts",
          "libs/web/shared/ui/input/src/**/*.html"
        ]
      },
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/web/shared/ui/input"],
      "options": {
        "jestConfig": "libs/web/shared/ui/input/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["type:ui", "scope:web"]
}
